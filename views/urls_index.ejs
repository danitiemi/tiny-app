<!DOCTYPE html>
<html lang="en">
<head>
<% include partials/_header.ejs %>
</head>
<body>
    <!-- Add your EJS code here -->
    <% for (url in urls) { %>
        <li>
    <%= url %>
    <%= urls[url] %>
        <form method="POST" action="/urls/<%= url %>/delete">
            <input type=submit value=delete>
        </form>

        <form method="GET" action="/urls/<%= url %>">
            <input type=submit value=edit>
        </form>
        </li>
    <% }; %>
</body>
</html>

